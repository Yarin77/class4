import java.util.Scanner;
public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("enter a number");
    int num = sc.nextInt();
    if (num > 0)
      System.out.println("positive");
    else
      System.out.println("negative");
  }
}



import java.util.Scanner;
public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("enter a number");
    int num = sc.nextInt();
    System.out.println("enter a number");
    int num2 = sc.nextInt();
    if (num < num2)
      System.out.println(num);
    else
      System.out.println(num2);
  }
}



import java.util.Scanner;
public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("enter the v of car a");
    int v1 = sc.nextInt();
    System.out.println("enter the v of car b");
    int v2 = sc.nextInt();
    System.out.println("enter the x of car a");
    double x1 = sc.nextDouble();
    System.out.println("enter the x of car b");
    double x2 = sc.nextDouble();
    double t1 = x1 / v1;
    double t2 = x2 / v2;
    if (t1 < t2) 
      System.out.println("a");
    else 
      System.out.println("b");
  }
}



import java.util.Scanner;
public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("enter a grade 1");
    int g1 = sc.nextInt();
    System.out.println("enter a grade 2");
    int g2 = sc.nextInt();
    System.out.println("enter a grade 3");
    int g3 = sc.nextInt();
    double av = (g1 + g2 + g3) / 3;
    System.out.println(av);
  }
}



import java.util.Scanner;
public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("enter a number");
    int num = sc.nextInt();
    System.out.println("enter a number");
    int num2 = sc.nextInt();
    System.out.println("enter a number");
    int num3 = sc.nextInt();
    if (num < num2 && num < num3)
      System.out.println(num);
    else if (num2 < num && num2 < num3)
      System.out.println(num2);
    else
      System.out.println(num3);
  }
}



import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("enter a the y of point 1");
    int y1 = sc.nextInt();
    System.out.println("enter a the x of point 1");
    int x1 = sc.nextInt();
    System.out.println("enter a the y of point 2");
    int y2 = sc.nextInt();
    System.out.println("enter a the x of point 2");
    int x2 = sc.nextInt();
    double distance = Math.sqrt(Math.pow(y2 - y1, 2) + Math.pow(x2 - x1, 2));
    System.out.println(distance);
  }
}
